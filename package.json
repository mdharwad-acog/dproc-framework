{
  "name": "@aganitha/dproc-framework",
  "version": "1.0.1",
  "description": "A plugin-based pipeline framework for LLM-powered data processing and report generation.",
  "type": "module",
  "main": "./dist/core/index.js",
  "types": "./dist/core/index.d.ts",
  "bin": {
    "dproc": "./bin/dproc-framework.js"
  },
  "files": [
    "dist/**/*",
    "bin/**/*",
    "web/.next/**/*",
    "web/public/**/*",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    }
  },
  "scripts": {
    "build": "pnpm clean && pnpm build:framework && pnpm --filter web build",
    "build:framework": "tsc -p tsconfig.cli.json",
    "dev": "pnpm --filter web dev",
    "start": "pnpm --filter web start",
    "dev:cli": "tsx src/cli/index.ts",
    "test": "echo 'Tests pending' && exit 0",
    "clean": "rm -rf dist web/.next *.tgz",
    "prepublishOnly": "pnpm build",
    "verify": "npm pack --dry-run"
  },
  "keywords": [
    "llm",
    "pipeline",
    "framework",
    "automation",
    "reports"
  ],
  "author": {
    "name": "mdharwad-acog",
    "email": "mdharwad@aganitha.ai"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdharwad-acog/dproc-framework.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.24.0",
  "dependencies": {
    "@ai-sdk/google": "^2.0.44",
    "@ai-sdk/openai": "^2.0.77",
    "@mdx-js/mdx": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "ai": "^5.0.108",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "debug": "^4.4.3",
    "inquirer": "^13.0.2",
    "nunjucks": "^3.2.4",
    "ora": "^9.0.0",
    "playwright": "^1.57.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/node": "^24.10.1",
    "@types/nunjucks": "^3.2.6",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^11.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  }
}