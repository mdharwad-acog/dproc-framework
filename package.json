{
  "name": "llm-data-framework",
  "version": "0.1.0",
  "description": "Generic data to LLM-powered structured reports",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "llm-framework": "./bin/llm-framework.js"
  },
  "scripts": {
    "test": "pnpm build && node --test --experimental-test-coverage tests/**/*.test.js",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "build": "tsc && pnpm copy-templates",
    "copy-templates": "mkdir -p dist/templates && cp -r src/templates/builtin dist/templates/",
    "dev": "tsx src/cli/index.ts",
    "prepublishOnly": "pnpm clean && pnpm build && pnpm test"
  },
  "keywords": [
    "llm",
    "reports",
    "data-processing",
    "ai"
  ],
  "author": "mdharwad-acog",
  "license": "MIT",
  "packageManager": "pnpm@10.24.0",
  "dependencies": {
    "@ai-sdk/google": "^2.0.44",
    "@ai-sdk/openai": "^2.0.77",
    "ai": "^5.0.108",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "csv-parse": "^6.1.0",
    "debug": "^4.4.3",
    "dotenv": "^17.2.3",
    "inquirer": "^13.0.2",
    "marked": "^17.0.1",
    "nunjucks": "^3.2.4",
    "ora": "^9.0.0",
    "playwright": "^1.57.0",
    "yaml": "^2.8.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/node": "^24.10.1",
    "@types/nunjucks": "^3.2.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
